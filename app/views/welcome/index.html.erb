      <input name="utf8" type="hidden" value="✓"></input>
      <input name="authenticity_token" type="hidden"></input>
      <div class="field">
        <label for="user_email">Email</label>
        <br/>
        <input autofocus="autofocus" class="" id="user_email" name="user[email]" type="email"></input>
      </div>
      <div class="field">
        <label for="user_password">Password</label>
        <em>(8 characters minimum)</em>
        <br/>
        <input autocomplete="off" class="" id="user_password" name="user[password]" type="password"></input>
      </div>
      <div class="field">
        <label for="user_password_confirmation">Password confirmation</label>
        <br/>
        <input autocomplete="off" class="" id="user_password_confirmation" name="user[password_confirmation]" type="password"></input>
      </div>
      <div class="actions">
        <input class="btn btn-default" name="commit" type="submit" value="Sign up"></input>
      </div>
    </form>
