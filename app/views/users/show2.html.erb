      <div class='col-xs-12 col-sm-12 col-md-6 col-lg-6 col-xs-offset-0 col-sm-offset-0 col-md-offset-3 col-lg-offset-3 toppad'>
        <div class='panel panel-info'>
          <div class='panel-heading'>
            <h3 class='panel-title'><%= current_user.name %></h3>
          </div>
          <div class='panel-body'>
            <div class='row'>
              <div align='center' class='col-md-3 col-lg-3'>
                <img alt='User Pic' class='img-circle img-responsive' src='http://babyinfoforyou.com/wp-content/uploads/2014/10/avatar-300x300.png'>
              </div>
              <!--
                <div class="col-xs-10 col-sm-10 hidden-md hidden-lg"> <br>
                <dl>
                <dt>DEPARTMENT:</dt>
                <dd>Administrator</dd>
                <dt>HIRE DATE</dt>
                <dd>11/12/2013</dd>
                <dt>DATE OF BIRTH</dt>
                <dd>11/12/2013</dd>
                <dt>GENDER</dt>
                <dd>Male</dd>
                </dl>
                </div>
              -->
              <div class='col-md-9 col-lg-9'>
                <table class='table table-user-information'>
                  <tbody>
                    <tr>
                      <td>Last name:</td>
                      <td><%= current_user.last_name %>
                      </td>
                    </tr>
                    <tr>
                      <td>Second last name:</td>
                      <td><%= current_user.slast_name %>
                      </td>
                    </tr>
                    <tr>
                      <td>Age</td>
                      <td><%= current_user.age %>
                      </td>
                    </tr>
                    <tr>
                      <td>Created at:</td>
                      <td><%= current_user.confirmed_at %>
                      </td>
                    </tr>
                    <tr>
                      <td>Email</td>
                      <td><%= current_user.email %>
                        <!-- %a{:href => "mailto:info@support.com"} info@support.com -->
