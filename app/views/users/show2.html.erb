                </table>
                <!-- %a.btn.btn-primary{:href => "#"} My Sales Performance -->
                <!-- %a.btn.btn-primary{:href => "#"} Team Sales Performance -->
              </div>
            </div>
          </div>
          <div class='panel-footer'>
            <a class='btn btn-sm btn-primary' data-original-title='Broadcast Message' data-toggle='tooltip' type='button'>
              <i class='glyphicon glyphicon-envelope'></i>
            </a>
            <span class='pull-right'>
              <a class='btn btn-sm btn-warning'>
                <i class='glyphicon glyphicon-edit'></i>
              </a>
              <a class='btn btn-sm btn-danger' data-original-title='Remove this user' data-toggle='tooltip' type='button'>
